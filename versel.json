{
    "version": 2,
    "builds": [
      {
        "src": "src/server.ts",
        "use": "@vercel/node",
        "config": {
          "includeFiles": ["package.json", "package-lock.json"]
        }
      }
    ],
    "routes": [
      {
        "src": "/.*",
        "dest": "/src/server.ts"
      }
    ],
    "env": {
      "NODE_ENV": "production"
    }
  }
  